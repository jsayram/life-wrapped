// -----------------------------------------------------------------------------
// CloudKit (Optional - Phase 2)
// -----------------------------------------------------------------------------
// Your CloudKit container identifier for cross-device sync
// Example: iCloud.com.yourcompany.lifewrapped
CLOUDKIT_CONTAINER_ID = 

// -----------------------------------------------------------------------------
// App Configuration
// -----------------------------------------------------------------------------
// Bundle identifier prefix (your team's reverse domain)
BUNDLE_ID_PREFIX = com.yourcompany

// App Group identifier for sharing data between app and extensions
// Example: group.com.yourcompany.lifewrapped
APP_GROUP_ID = 

// -----------------------------------------------------------------------------
// Development Team
// -----------------------------------------------------------------------------
// Your Apple Developer Team ID (found in Apple Developer portal)
// Example: ABCD1234EF
DEVELOPMENT_TEAM = 

// -----------------------------------------------------------------------------
// Future API Keys (if ever needed)
// -----------------------------------------------------------------------------
// These are placeholders for any future external services
// Remember: Privacy-first means these should rarely be needed

// ANALYTICS_KEY = 
// CRASH_REPORTING_KEY = 

// -----------------------------------------------------------------------------
// Feature Flags (Development)
// -----------------------------------------------------------------------------
// Enable experimental features during development
ENABLE_DEBUG_FEATURES = YES
ENABLE_MOCK_AUDIO = NO
